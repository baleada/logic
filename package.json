{
  "name": "@baleada/logic",
  "version": "0.20.11",
  "description": "UI logic for the Baleada toolkit",
  "main": "lib/index.cjs",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "require": "./lib/index.cjs",
      "import": "./lib/index.js"
    }
  },
  "scripts": {
    "prepare": "tsc && rollup --config rollup.config.ts",
    "test:coverage": "node -r esbuild-register scripts/testCoverage.ts",
    "test:only": "run() { uvu -r esbuild-register tests/$2 $1.test.ts$; }; run",
    "test:node": "npm run test:only . node",
    "test:browser": "npm run test:only . browser",
    "test": "npm run test:only .",
    "dev": "vite"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/baleada/logic.git"
  },
  "keywords": [
    "baleada",
    "logic",
    "user interface",
    "user interface logic",
    "class",
    "pipe"
  ],
  "author": {
    "name": "Alex Vipond",
    "email": "hello@alexvipond.dev",
    "url": "https://alexvipond.dev"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/baleada/logic/issues"
  },
  "homepage": "https://baleada.netlify.com",
  "devDependencies": {
    "@baleada/animateable-utils": "^0.0.2",
    "@baleada/prepare": "^0.5.11",
    "@types/node": "^14.14.41",
    "@vue/compiler-sfc": "^3.2.9",
    "esbuild": "^0.9.7",
    "esbuild-register": "^2.6.0",
    "rollup": "^2.56.3",
    "tailwindcss": "^2.1.1",
    "typescript": "^4.4.3",
    "uvu": "^0.5.1",
    "vite": "^2.4.4",
    "vue": "^3.2.12",
    "vue-router": "^4.0.3"
  },
  "dependencies": {
    "@babel/runtime": "^7.14.8",
    "@sindresorhus/slugify": "^2.1.0",
    "@snigo.dev/color": "^0.0.6",
    "@types/dompurify": "^2.2.3",
    "@types/requestidlecallback": "^0.3.1",
    "@types/resize-observer-browser": "^0.1.5",
    "bezier-easing": "^2.1.0",
    "dompurify": "^2.2.6",
    "fast-fuzzy": "^1.10.8",
    "lazy-collections": "^0.8.0",
    "perfect-freehand": "^1.0.4",
    "polygon-clipping": "^0.15.3"
  },
  "sideEffects": false
}
