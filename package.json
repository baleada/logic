{
  "name": "@baleada/logic",
  "version": "0.15.21",
  "description": "UI logic for the Baleada toolkit",
  "type": "module",
  "main": "lib/index.cjs",
  "module": "lib/index.js",
  "exports": {
    ".": {
      "require": "./lib/index.cjs",
      "import": "./lib/index.js"
    },
    "./metadata": {
      "require": "./metadata/index.cjs",
      "import": "./metadata/index.js"
    }
  },
  "scripts": {
    "prepare": "rollup --config rollup.config.js",
    "test:only": "run() { npx uvu tests/$2 $1.test.js$; }; run",
    "test": "npm run test:only .",
    "dev:test": "cd tests/stubs/app && npm run dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/baleada/logic.git"
  },
  "keywords": [
    "baleada",
    "logic",
    "user interface",
    "user interface logic",
    "class",
    "subclass"
  ],
  "author": {
    "name": "Alex Vipond",
    "email": "hello@alexvipond.dev",
    "url": "https://alexvipond.dev"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/baleada/logic/issues"
  },
  "homepage": "https://baleada.netlify.com",
  "devDependencies": {
    "@baleada/prepare": "^0.3.6",
    "rollup": "^2.33.3",
    "uvu": "^0.5.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.1",
    "bezier-easing": "^2.1.0",
    "fast-fuzzy": "^1.10.8",
    "lodash-es": "^4.17.15",
    "mix-css-color": "^0.1.1"
  },
  "sideEffects": false
}
