{
  "name": "@baleada/logic",
  "version": "0.19.9",
  "description": "UI logic for the Baleada toolkit",
  "main": "lib/index.cjs",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "require": "./lib/index.cjs",
      "import": "./lib/index.js"
    },
    "./metadata": {
      "require": "./metadata/index.cjs",
      "import": "./metadata/index.js"
    }
  },
  "scripts": {
    "metadata": "node scripts/metadata.js && tsc --project tsconfig.metadata.json",
    "prepare": "npm run metadata && tsc && rollup --config rollup.config.ts",
    "test:only": "run() { uvu -r ts-node/register tests/$2 $1.test.ts$; }; run",
    "test": "npm run test:only .",
    "dev": "vite"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/baleada/logic.git"
  },
  "keywords": [
    "baleada",
    "logic",
    "user interface",
    "user interface logic",
    "class",
    "pipe"
  ],
  "author": {
    "name": "Alex Vipond",
    "email": "hello@alexvipond.dev",
    "url": "https://alexvipond.dev"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/baleada/logic/issues"
  },
  "homepage": "https://baleada.netlify.com",
  "devDependencies": {
    "@baleada/animateable-utils": "^0.0.2",
    "@baleada/prepare": "^0.5.1",
    "@types/node": "^14.14.41",
    "@vue/compiler-sfc": "^3.0.5",
    "rollup": "^2.33.3",
    "tailwindcss": "^2.1.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3",
    "uvu": "^0.5.1",
    "vite": "^2.1.5",
    "vue": "^3.0.5",
    "vue-router": "^4.0.3"
  },
  "dependencies": {
    "@babel/runtime": "^7.13.17",
    "@sindresorhus/slugify": "^2.1.0",
    "@snigo.dev/color": "^0.0.6",
    "@types/dompurify": "^2.2.1",
    "@types/requestidlecallback": "^0.3.1",
    "@types/resize-observer-browser": "^0.1.5",
    "bezier-easing": "^2.1.0",
    "dompurify": "^2.2.6",
    "fast-fuzzy": "^1.10.8",
    "lazy-collections": "^0.7.1",
    "rfdc": "^1.3.0"
  },
  "sideEffects": false
}
