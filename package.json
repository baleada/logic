{
  "name": "@baleada/logic",
  "version": "0.22.5",
  "description": "UI logic for the Baleada toolkit",
  "main": "lib/index.cjs",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "require": "./lib/index.cjs",
      "import": "./lib/index.js"
    }
  },
  "scripts": {
    "prepare": "tsc && rollup --config rollup.config.ts --configPlugin typescript --bundleConfigAsCjs",
    "test:coverage": "node -r esbuild-register scripts/testCoverage.ts",
    "test:only": "run() { uvu -r esbuild-register tests/$2 $1.test.ts$; }; run",
    "test:node": "npm run test:only . node",
    "test:browser": "npm run test:only . browser",
    "test": "npm run test:only .",
    "dev": "vite"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/baleada/logic.git"
  },
  "keywords": [
    "baleada",
    "logic",
    "user interface",
    "user interface logic",
    "class",
    "pipe"
  ],
  "author": {
    "name": "Alex Vipond",
    "email": "hello@alexvipond.dev",
    "url": "https://alexvipond.dev"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/baleada/logic/issues"
  },
  "homepage": "https://baleada.dev",
  "devDependencies": {
    "@baleada/prepare": "^0.5.30",
    "@types/node": "^14.14.41",
    "@vitejs/plugin-vue": "^3.2.0",
    "esbuild": "^0.15.13",
    "esbuild-register": "^3.3.3",
    "rollup": "^3.2.5",
    "tslib": "^2.4.1",
    "typescript": "^4.8.4",
    "uvu": "^0.5.6",
    "vite": "^3.2.5",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "dependencies": {
    "@babel/runtime": "^7.20.6",
    "@sindresorhus/slugify": "^2.2.0",
    "@snigo.dev/color": "^0.0.6",
    "@types/dompurify": "^2.4.0",
    "@types/requestidlecallback": "^0.3.5",
    "@types/resize-observer-browser": "^0.1.7",
    "bezier-easing": "^2.1.0",
    "dompurify": "^2.4.1",
    "fast-fuzzy": "^1.12.0",
    "lazy-collections": "^0.10.0",
    "perfect-freehand": "^1.2.0",
    "polygon-clipping": "^0.15.3"
  },
  "sideEffects": false
}
