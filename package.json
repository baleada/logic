    "@babel/core": "^7.5.4",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-proposal-private-methods": "^7.4.4",
    "@babel/preset-env": "^7.5.4",
    "@babel/runtime-corejs3": "^7.5.4",
    "ava": "^2.2.0",
    "babel-eslint": "^10.0.2",
    "core-js": "^3.1.4",
    "eslint": "^6.0.1",
