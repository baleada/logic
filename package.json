{
  "name": "@baleada/logic",
  "version": "0.18.0",
  "description": "UI logic for the Baleada toolkit",
  "type": "module",
  "main": "lib/index.cjs",
  "module": "lib/index.js",
  "exports": {
    ".": {
      "require": "./lib/index.cjs",
      "import": "./lib/index.js"
    },
    "./metadata": {
      "require": "./metadata/index.cjs",
      "import": "./metadata/index.js"
    }
  },
  "scripts": {
    "prepare": "rollup --config rollup.config.js",
    "prepare:test": "rollup --config test.rollup.config.js",
    "test:only": "run() { npx uvu tests/$2 $1.test.js$; }; run",
    "test": "npm run test:only .",
    "dev": "vite"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/baleada/logic.git"
  },
  "keywords": [
    "baleada",
    "logic",
    "user interface",
    "user interface logic",
    "class",
    "subclass"
  ],
  "author": {
    "name": "Alex Vipond",
    "email": "hello@alexvipond.dev",
    "url": "https://alexvipond.dev"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/baleada/logic/issues"
  },
  "homepage": "https://baleada.netlify.com",
  "devDependencies": {
    "@baleada/animateable-utils": "^0.0.2",
    "@baleada/prepare": "^0.4.43",
    "@rollup/plugin-typescript": "^8.2.0",
    "@types/dompurify": "^2.2.1",
    "@types/resize-observer-browser": "^0.1.5",
    "@vue/compiler-sfc": "^3.0.5",
    "npm": "^7.6.3",
    "rollup": "^2.33.3",
    "tailwindcss": "^2.0.1-compat",
    "typescript": "^4.2.3",
    "uvu": "^0.5.0",
    "vite": "^2.0.0-beta.36",
    "vue": "^3.0.5",
    "vue-router": "^4.0.3"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.1",
    "@sindresorhus/slugify": "^1.1.0",
    "bezier-easing": "^2.1.0",
    "dompurify": "^2.2.6",
    "fast-fuzzy": "^1.10.8",
    "mix-css-color": "^0.1.1"
  },
  "sideEffects": false
}
