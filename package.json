{
  "name": "@baleada/logic",
  "version": "0.0.0",
  "description": "UI logic for the Baleada toolkit",
  "main": "lib/index.js",
  "scripts": {
    "index": "node scripts/buildIndices.js",
    "empty-lib": "node scripts/emptyDir.js 'lib'",
    "babelify": "npm run empty-lib && npx babel src --out-dir lib",
    "empty-browser-lib": "node scripts/emptyDir.js 'browser-lib'",
    "browser-babelify": "npm run empty-browser-lib && npx babel src --config-file ./browser-babel.config.js --out-dir browser-lib",
    "empty-test-fixtures-classes": "node scripts/emptyDir.js 'tests/fixtures/classes'",
    "empty-test-fixtures-subclasses": "node scripts/emptyDir.js 'tests/fixtures/subclasses'",
    "empty-test-fixtures-all": "npm run empty-test-fixtures-classes && npm run empty-test-fixtures-subclasses",
    "browserify-classes": "node scripts/browserify.js 'classes'",
    "browserify-subclasses": "node scripts/browserify.js 'subclasses'",
    "browserify-all": "npm run browserify-classes && npm run browserify-subclasses",
    "browserify": "npm run browser-babelify && npm run empty-test-fixtures-all && npm run browserify-all",
    "test": "npm run browserify && ava",
    "prepare": "npm run index && npm run babelify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/baleada/logic.git"
  },
  "keywords": [
    "baleada",
    "logic",
    "user interface",
    "user interface logic",
    "class",
    "subclass"
  ],
  "author": {
    "name": "Alex Vipond",
    "email": "hello@alexvipond.dev",
    "url": "https://alexvipond.dev"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/baleada/logic/issues"
  },
  "homepage": "https://baleada.netlify.com",
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-proposal-private-methods": "^7.4.4",
    "@babel/preset-env": "^7.5.4",
    "@babel/runtime-corejs3": "^7.5.4",
    "@nuxtjs/eslint-config": "^1.1.2",
    "ava": "^2.4.0",
    "babel-eslint": "^10.0.2",
    "core-js": "^3.1.4",
    "eslint": "^6.0.1",
    "eslint-config-standard": "^13.0.1",
    "eslint-plugin-ava": "^7.1.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-nuxt": "^0.4.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "puppeteer": "^1.18.1",
    "regenerator-runtime": "^0.13.3"
  },
  "dependencies": {
    "@shopify/draggable": "^1.0.0-beta.8",
    "animejs": "^3.0.1",
    "gotrue-js": "^0.9.25",
    "hammerjs": "^2.0.8",
    "lunr": "^2.3.6",
    "luxon": "^1.17.1",
    "markdown-it": "^10.0.0",
    "popper.js": "^1.15.0",
    "pressure": "^2.1.2"
  },
  "sideEffects": "false"
}
